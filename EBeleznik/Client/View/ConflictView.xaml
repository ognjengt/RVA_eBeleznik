<Window x:Class="Client.View.ConflictView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client.View"
        mc:Ignorable="d"
        Title="ConflictView" Height="233.232" Width="379.573">
    <Grid Margin="0,0,2,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="67*"/>
            <ColumnDefinition Width="118*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label" FontSize="15" FontWeight="Bold" Content="Doslo je do konflikta!" HorizontalAlignment="Left" Margin="102,10,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <Label x:Name="label_Copy" Content="Neko je u medjuvremenu izmenio ovu belesku" HorizontalAlignment="Left" Margin="56,45,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <Button x:Name="button" Command="{Binding pregaziIzmeneCommand}" Content="Pregazi tudje izmene" HorizontalAlignment="Left" Margin="36,136,0,0" VerticalAlignment="Top" Width="129" Height="24" Grid.ColumnSpan="2"/>
        <Button x:Name="button_Copy" Command="{Binding odbaciSvojeCommand}" Content="Odbaci svoje izmene" HorizontalAlignment="Left" Margin="71.89,136,0,0" VerticalAlignment="Top" Width="139" Height="24" Grid.Column="1"/>

    </Grid>
</Window>
